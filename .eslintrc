{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-closing-bracket-location": [2, "after-props"],
    "react/display-name": 0,
    "react/jsx-tag-spacing": [2, {"beforeSelfClosing": "always"}],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/jsx-curly-spacing": [2, {"when": "never", "children": true}],
    "react/jsx-equals-spacing": [2, "never"],
    "no-cond-assign": [2, "always"],
    "no-console": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true }],
    "use-isnan": 2,
    "valid-typeof": 2,
    "accessor-pairs": [1, {"setWithoutGet": true}],
    "block-scoped-var": 2,
    "complexity": 2,
    "default-case": 2,
    "dot-notation": 1,
    "dot-location": ["error", "property"],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 1,
    "no-else-return": 2,
    "no-labels": 2,
    "no-eq-null": 2,
    "no-eval": 1,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [1, {"boolean": true, "number": true, "string": true}],
    "no-implied-eval": 1,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal": 2,
    "no-param-reassign": [2, {"props": true}],
    "no-process-env": 1,
    "no-proto": 2,
    "no-redeclare": [2, {"builtinGlobals": true}],
    "no-return-assign": [2, "always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "wrap-iife": 1,
    "yoda": [1, "never", {}],
    "init-declarations": [2, "always"],
    "no-catch-shadow": 1,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [2, {"hoist": "all"}],
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "callback-return": 0,
    "handle-callback-err": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "array-bracket-spacing": ["error", "never", {
      "singleValue": false,
      "objectsInArrays": false,
      "arraysInArrays": true
    }],
    "brace-style": 2,
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": 1,
    "computed-property-spacing": ["error", "never"],
    "id-length": [1,{"min":2,"max":35,"exceptions":["_","t","e"]}],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "new-cap": [1,{"newIsCap":true,"capIsNew":true}],
    "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
    "no-multiple-empty-lines": [1,{"max":1}],
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-unneeded-ternary": 1,
    "operator-linebreak": [1,"before"],
    "linebreak-style": [2, "unix"],
    "padded-blocks": [1, "never"],
    "prefer-template": ["warn"],
    "quotes": [2, "single"],
    "semi-spacing": 1,
    "semi": [2, "always"],
    "sort-vars": 1,
    "space-before-blocks": ["error", "always"],
    "space-in-parens": ["error", "never"],
    "object-curly-spacing": ["error", "always", {
      "arraysInObjects": false,
      "objectsInObjects": false
    }],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "always"
    }],
    "space-unary-ops": [1, {"words": true, "nonwords": false}],
    "template-curly-spacing": ["error", "never"],
    "wrap-regex": 1,
    "require-await": 2,
    "constructor-super": 1,
    "no-class-assign": 1,
    "no-const-assign": 2,
    "no-this-before-super": 1,
    "no-var": 2,
    "jsx-quotes": [1, "prefer-double"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true, "mode": "strict"}],
    "no-lonely-if": 2,
    "no-trailing-spaces": 2,
    "space-infix-ops": 2,
    "keyword-spacing": 2,
    "arrow-spacing": [1,{"after":true, "before":false}],
    "arrow-parens": 2,
    "eol-last": ["error", "always"]
  }
}
